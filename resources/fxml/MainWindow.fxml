        </Button>
        <Button fx:id="reloadButton" layoutX="402.0" layoutY="247.0" mnemonicParsing="false" onAction="#reloadEvent"
                prefHeight="34.0" prefWidth="80.0" text="重载">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>
        <Button fx:id="openWallpaperDirButton" layoutX="164.0" layoutY="247.0" mnemonicParsing="false"
                onAction="#openWallpaperDirEvent" prefHeight="34.0" prefWidth="114.0" text="打开壁纸目录">
