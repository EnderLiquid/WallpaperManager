                <Font size="15.0"/>
            </font>
        </Button>
        <Button fx:id="nextButton" layoutX="402.0" layoutY="44.0" mnemonicParsing="false" onAction="#nextEvent"
                prefHeight="34.0" prefWidth="80.0" text="上一张">
